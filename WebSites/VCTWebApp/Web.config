    <add key="LinkedServerConnectionString" value="r5IJ/jyd79RvQRR8lKBhaBaEj8QcnNrXPhu8Z1UxiaG6NZhaSHQ7uK0t0OgGaqnvGHnCiK/smjTAN/YtuqLre120IjtI6AFd6vOTsQjISwlXWkyxPYqkMw=="/>
    <add key="DefaultCulture" value="en-US"/>
    <add key="ConnectTimeout" value="50"/>

    <add key="EmailServer" value="webmail.irissoftware.com"/>
    <add key="EmailPort" value="587"/>
    <add key="SenderEmailId" value="IRIS.ITS.Alerts@irissoftware.com"/>
